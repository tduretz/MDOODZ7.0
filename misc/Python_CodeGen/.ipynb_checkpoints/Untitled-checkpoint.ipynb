    "from sympy import *\n",
    "import numpy as np \n",
    "init_printing()\n",
    "\n",
    "Exx,Ezz,Exz,A1pwl,npwl = symbols('Exx,Ezz,Exz,A1pwl,npwl')\n",
    "Eii      = (0.5*Exx*Exx + 0.5*Ezz*Ezz + Exz*Exz)**(1.0/2.0)\n",
    "eta_pwl  = A1pwl * Eii**(1.0/npwl - 1.0 );"
   ]
  }

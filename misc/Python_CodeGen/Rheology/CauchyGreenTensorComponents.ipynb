   ],
   "source": [
    "from sympy import *\n",
    "\n",
    "Fxx, Fxz, Fzx, Fzz = symbols('Fxx, Fxz, Fzx, Fzz ')\n",
    "F         = Matrix(2,2,[Fxx,Fxz,Fzx,Fzz])\n",
    "# Left Cauchy-Green\n",
    "CG = F.transpose() * F\n",
    "CG"
   ]
